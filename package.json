{
  "name": "angular-ssr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr-no-universal": "./node_modules/.bin/ts-node -O='{\"module\":\"commonjs\"}' ./server-no-universal.ts",
    "build": "ng build --prod",
    "prerender-no-universal": "./node_modules/.bin/ts-node -O='{\"module\":\"commonjs\"}' ./prerender-no-universal.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr:w": "tsc-watch --onSuccess \"node ./server.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc -p tsconfig.server.json",
    "serve:ssr": "./node_modules/.bin/ts-node -O='{\"module\":\"commonjs\"}' ./server.ts",
    "serve:ssr:js": "node dist/server",
    "build:ssr": "ng build --prod && ng run angular-ssr:server:production",
    "build:ssr:webpack": "npm run build:ssr && npm run compile:server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/platform-server": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "compression": "^1.7.4",
    "core-js": "^2.5.4",
    "express": "^4.17.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@angular/language-service": "~9.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tsc-watch": "^2.2.1",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3",
    "webpack-cli": "^3.1.0"
  }
}
